public class convert {
	
	    public static void main(String[] args)
	    {


	        double minInYear = 60 * 24 * 365;

	        double min = 3456789;

	        long years = (long) (min / minInYear);
	        int days = (int) (min / 60 / 24) % 365;

	        System.out.println((int) min + " minutes is " + years + " years and " + days + " days");
	    }
	}
