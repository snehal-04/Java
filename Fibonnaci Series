import java.util.Scanner;
public class fibonacci
{
	
	    public static void main(String[] args) 
	    {
	        int n;
	        int a = 0;
	        int b = 0;
	        int c = 1;
	        Scanner s = new Scanner(System.in);
	        System.out.print("Enter number of digits you want in your series:");
	        n = s.nextInt();
	        
	        System.out.print("Fibonacci Series:");
	        for(int i = 1; i <= n; i++)
	        {
	            a = b;
	            b = c;
	            c = a + b;
	            System.out.print(a+" ");
	        }
	    }
	}
